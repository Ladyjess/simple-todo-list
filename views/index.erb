<div class="jumbotron"><h1> To do list</h1></div>

<p>Here are your tasks:</p>

<ul>
  <% @tasks.each do |whatever|  %>
  <li> <%= whatever.description %> </li>

  <% end %>

</ul>


<div class="form-group">
  <form action="/whatever" method="post">

  <label for="description"> Create a new task:</label>
  <input id="description" name="description" class="form-control" type="text">

  <button type="submit" class="btn btn-warning">Add it! </button>
</form>
</div>
